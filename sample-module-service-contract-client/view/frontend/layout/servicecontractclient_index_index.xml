<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Sample Service Contract Client</title>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument name="title" translate="true" xsi:type="string">Sample Service Contract Client</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\SampleServiceContractClient\Block\ProductList" name="servicecontractclient"
                   template="Magento_SampleServiceContractClient::service_contract_client.phtml"/>
        </referenceContainer>
    </body>
</page>
